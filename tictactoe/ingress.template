kind: Ingress
metadata:
  name: wap-ingress
  annotations:
    ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: tictactoe.domain.placeholder
    http:
      paths:
      - path: /
        backend:
          serviceName: tictactoe
          servicePort: 80
